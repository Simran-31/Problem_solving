#include <iostream>

using namespace std;

int main()
{
    string s;
    cin>>s;
    int a=0,b=0,c=0,d=0;
    for(int i=0;i<s.length();i++){
        if(islower(s[i])){
            a++;
        }
        else if(isupper(s[i])){
            b++;
        }
        else if(isdigit(s[i])){
            c++;
        }
        else{
            d++;
        }
        
    }
    cout<<a*b+c*d<<endl;

    return 0;
}
